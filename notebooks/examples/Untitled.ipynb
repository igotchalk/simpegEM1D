    "import ipysheet\n",
    "import ipywidgets as widgets\n",
    "\n",
    "sheet = ipysheet.sheet()\n",
    "\n",
    "button = widgets.Button(description='Add Row')\n",
    "out = widgets.Output()\n",
    "def add_row(_):\n",
    "    with out:\n",
    "        sheet.rows += 1\n",
    "\n",
    "button.on_click(add_row)\n",
    "\n",
    "widgets.VBox([button,sheet])"
   ]
